{
  "name": "@codebuff/code-map",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "bun": "./src/*.ts",
      "import": "./src/*.ts",
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test"
  },
  "sideEffects": false,
  "engines": {
    "bun": "^1.3.0"
  },
  "dependencies": {
    "@vscode/tree-sitter-wasm": "0.1.4",
    "web-tree-sitter": "0.25.6"
  },
  "devDependencies": {
    "@types/node": "22",
    "@types/bun": "^1.3.0"
  }
}
